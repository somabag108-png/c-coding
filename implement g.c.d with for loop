#include <stdio.h>

int main() {
    int a, b, gcd = 1;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    // Loop from 1 to the smaller number
    for (int i = 1; i <= a && i <= b; i++) {
        if (a % i == 0 && b % i == 0) {
            gcd = i;  // store the common divisor
        }
    }

    printf("GCD of %d and %d is %d\n", a, b, gcd);

    return 0;
}
